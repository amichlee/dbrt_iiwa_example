<launch>

  <arg name="config" default="$(find dbrt_example_iiwa)/config/fusion_tracker_cpu.yaml" />

  <node name="fusion_tracker" type="fusion_tracker" pkg="dbrt" output="screen" respawn="false" launch-prefix="">
    <param name="robot_description_package_path" type="string" value="$(find iiwa_robot_model2)" />
    <rosparam command="load" file="$(arg config)" />
    <rosparam command="load" file="$(find dbrt_example_iiwa)/config/camera.yaml" />
  </node>

</launch>

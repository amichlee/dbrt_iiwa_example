

<launch>
  <arg name="model" default="$(find iiwa_robot_model)/models/kuka_iiwa_real.urdf"/>


  <param name="use_sim_time" value="true" />
    <param name="robot_description" command="$(find xacro)/xacro.py $(arg model)  material_color:='0.85 0.2 0.2 1.0'"/>
    <param name="robot_description_blue" command="$(find xacro)/xacro.py $(arg model) material_color:='0.2 0.2 0.85 1.0'" />


  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find flows)/config/visualization.rviz"/>

</launch>
